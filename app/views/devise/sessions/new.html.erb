<!-- Login -->
<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8">
		<title>QMi Login</title>
		<meta http-equiv="content-type" content="text/html; charset=utf-8" />
		<meta name="description" content="" />
		<meta name="keywords" content="" />
		<!--[if lte IE 8]><script src="js/html5shiv.js"></script><![endif]-->
		<script src="js/jquery.min.js"></script>
		<script src="js/skel.min.js"></script>
		<script src="js/skel-layers.min.js"></script>
		<script src="js/init.js"></script>
		<noscript>
			<link rel="stylesheet" href="css/skel.css" />
			<link rel="stylesheet" href="css/style.css" />
			<link rel="stylesheet" href="css/style-xlarge.css" />
		</noscript>
	</head>
	<body id="landing">

    <section id="banner2">
      <h2>We're as ready as you are.</h2>

      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <%= f.email_field :email, :placeholder => 'email', :style => "text-align: center", autofocus: true %> <br />
        <%= f.password_field :password, :placeholder => 'password', :style => "text-align: center", autocomplete: "off" %> <br />

        <% if devise_mapping.rememberable? -%>
          <%= f.check_box :remember_me %>
          <%= f.label :remember_me %> <br />
        <% end -%>

        <%= f.submit "Log in", class: "btn-submit" %>
      <% end %>

      <%= render "devise/shared/links" %>

    </section>
    <section id="banner3">
    </section>


  </body>
</html>
